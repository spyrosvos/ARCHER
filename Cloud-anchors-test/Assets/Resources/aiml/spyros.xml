<category>
	<pattern>*</pattern>
	<template>
		<random>
			<li>I'm sorry I don't understand</li>
			<li>I beg your pardon?</li>
			<li>Could you explain that in different words?</li>
			<li>Huh?</li>
			<li>You aren't making sense to me...</li>
		</random>
	</template>
</category>

<category>
	<pattern>HELLO</pattern>
	<template>Hi there!$show sign1$pos sign1 5.3 2 -1$</template>
</category>

  <category>
    <pattern>HELLO THERE</pattern>
    <template>Hi there!$show sign1$pos sign1 3.1 1 2.5$</template>
  </category>

  <category>
		<pattern>HI</pattern>
		<template>Hello to you!$hide sign1$</template>
  </category>

  <category>
    <pattern>FACTORY *</pattern>
    <template><srai>FACTORY</srai></template>
  </category>

  <category>
    <pattern>* FACTORY</pattern>
    <template>
      <srai>FACTORY</srai>
    </template>
  </category>

  <category>
    <pattern>* FACTORY *</pattern>
    <template>
      <srai>FACTORY</srai>
    </template>
  </category>
  
  <category>
    <pattern>FACTORY</pattern>
    <template>This is the Anerousis Lead Factory</template>
  </category>
  

</aiml>
